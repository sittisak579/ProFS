<% include ../partials/header%>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
            <th>Password</th>
            <th><a  class="btn btn-danger" href="/newuser" role="button">ADD NEW</a></th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(users){ %>
        <tr>
            <td scope="row"><%= users.id %></td>
            <td><%= users.email %></td>
            <td><%= users.password %></td>
            <td><a  class="btn btn-primary" href="/users/<%= users.id %>" role="button">EDIT</a></td>
        </tr>
    <%}); %>
    </tbody>
</table>
<% include ../partials/footer%>