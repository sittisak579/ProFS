<% include ../partials/header%>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
    <% products.forEach(function(products){ %>
        <tr>
            <td scope="row"><%= products.id %></td>
            <td><%= products.title %></td>
            <td><%= products.price %></td>
            <td>
           <a class="btn btn-primary" href="/products/<%= products.id %>" role="button">Edit</a>
            </td>
        </tr>
    <%}); %>
    </tbody>
</table>

<% include ../partials/footer%>










