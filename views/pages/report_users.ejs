<% include ../partials/header%>
<h1 class="text-center">Report:Top 20 Customers by Sales</h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>title</th>
            <th>Quantity</th>
            <th>Total Sales</th>
        </tr>
    </thead>
    <tbody id="myTable">
    <% users.forEach(function(user) { %>
        <tr>
            <td scope="row"><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.title %></td>
            <td><%= user.quantity %></td>
            <td>$<%= user.total %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
<% include ../partials/footer%>