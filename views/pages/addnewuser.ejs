<% include ../partials/header %>
<p></p>
<form action ="/addnewuser" method = "post">
<div class="jumbotron bg-warning text-dark">
<h1 class="display-4">ADD NEW USER</h1>
<div class="form-group">
  <label for="">ID</label>
  <input type="number"
    class="form-control text-white bg-dark" name="user_id" id="" aria-describedby="helpId" placeholder="ID">
  <small id="helpId" class="form-text text-muted"></small>
</div>
<div class="form-group">
  <label for="">Email</label>
  <input type="email"
    class="form-control text-white bg-dark" name="email" id="" aria-describedby="helpId" placeholder="Email">
  <small id="helpId" class="form-text text-muted"></small>
</div>
<div class="form-group">
  <label for="">Password</label>
  <input type="password"
    class="form-control text-white bg-dark" name="password" id="" aria-describedby="helpId" placeholder="Password">
  <small id="helpId" class="form-text text-muted"></small>
</div>


<button type="submit" class="btn btn-primary">ADD NEWUSER</button>
<a name="" id="" class="btn btn-secondary" href="/users" role="button">CANCLE</a>


</div>
</form>

<% include ../partials/footer %>